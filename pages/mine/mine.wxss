page {
  background-color: #f2f2f2;
  font-family: Arial;
}

.mine-nav {
  height: 210rpx;
  background-color: #03a9f4;
  display: flex;
}

.mine-nav image {
  width: 135rpx;
  height: 135rpx;
  border-radius: 50%;
  margin: 40rpx 0rpx 40rpx 60rpx;
}

.mine-info {
  display: flex;
  flex-direction: column;
  font-size: 16px;
  color: #fff;
  font-weight: bold;
  margin-left: 50rpx;
  line-height: 60rpx;
  margin-top: 40rpx;
}

.mine-item {
  margin: 20rpx 0rpx;
}

.mine-item-col {
  height: 100rpx;
  background-color: #fff;
  margin: 4rpx 0rpx;
  display: flex;
  flex-direction: row;
  position: relative;
}

.mine-item-col image:first-child {
  width: 65rpx;
  height: 71rpx;
  margin: 20rpx 30rpx;
}

.mine-item-col image:last-child {
  width: 52rpx;
  height: 54rpx;
  position: absolute;
  top: 26rpx;
  right: 20rpx;
}

.mine-item-col text {
  margin-top: 35rpx;
  font-size: 14px;
}

.mine-item-quit {
  margin-top: 60rpx;
  height: 117rpx;
  background-color: #fff;
  display: flex;
  flex-direction: row;
}

.mine-item-quit image {
  width: 65rpx;
  height: 71rpx;
  margin: 20rpx 0rpx;
  margin-left: 270rpx;
}
.mine-item-quit text {
  margin-top: 35rpx;
  margin-left: 26rpx;
  font-size: 15px;
}