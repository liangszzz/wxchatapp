.profile-info {
  padding: 0 40rpx;
}

.profile-info-tile{
  font-size: 15px;
  height: 50rpx;
  color:#888888;
  font-family: PingFangSC-regular;
  font-weight: bold;
}

.profile-id {
  height: 310rpx;
  position: relative;
}

.profile-id image {
  width: 280rpx;
  height: 181rpx;
  border: 2rpx dashed gray;
  border-radius: 12rpx;
  text-align: center;
}

.profile-id image:first-child {
  position: absolute;
  top: 144rpx;
  left: 30%;
}


.next_btn{
  margin-top: 180rpx;
  font-size: 18px;
  background-color: #03a9f4;
  color:#fff;
}